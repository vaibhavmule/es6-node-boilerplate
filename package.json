{
  "name": "es6-node-boilerplate",
  "version": "1.0.0",
  "description": "es6 node boilerplate",
  "main": "build/index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register",
    "test-cov": "babel-node node_modules/isparta/bin/isparta cover --report html --report text node_modules/.bin/_mocha",
    "build": "babel src --out-dir build",
    "clean": "rm -rf build",
    "lint": "eslint src test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vaibhavmule/es6-node-boilerplate"
  },
  "author": "Vaibhav Mule",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/VaibhavMule/es6-node-boilerplate/issues"
  },
  "homepage": "https://gitlab.com/VaibhavMule/es6-node-boilerplate#README",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "eslint": "^2.5.1",
    "eslint-config-airbnb": "^6.2.0",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5"
  }
}
